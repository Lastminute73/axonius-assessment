<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Session_Title__c</fullName>
    <description>this field was created to be used in listviews to make it easier to see the trainer, duration, and training type.</description>
    <externalId>false</externalId>
    <formula>TEXT(Duration__c) &amp; &quot;-Minute &quot; &amp; TEXT (  Training_Package_Type__c ) &amp; &quot; Session&quot;</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Session Title</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
